+  strncat(cache_dir, "/crls", PATH_MAX);
+  if (mkdir_if_not_exists(data, cache_dir) == NULL)
+  {
+    return NULL;
+  }
+  strncat(cache_dir, "/", PATH_MAX);
+#elif defined(__APPLE__)
+  char *home_env = getenv("HOME");
+  if (home_env == NULL) {
+    return NULL;
+  }
+  strncpy(cache_dir, home_env, PATH_MAX);
