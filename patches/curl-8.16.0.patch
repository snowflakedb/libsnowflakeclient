+/**
+ * Initialize Check OCSP revocation status method
+ */
+SF_PUBLIC(CURLcode) initCertOCSP()
+{
+  /* must call only once. not thread safe */
+  _mutex_init(&ocsp_response_cache_mutex);
+  atexit(termCertOCSP);
+  return CURLE_OK;
+}
