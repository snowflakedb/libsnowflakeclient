+	-Wvtls/sf_crl.c -Wvtls/sf_crl.h $(CSOURCES) $(HHEADERS))
+
+all-local: $(CHECKSOURCES)
+
+# UNITPROTOS depends on every C file in the lib/ dir
+$(UNITPROTOS): $(CSOURCES)
