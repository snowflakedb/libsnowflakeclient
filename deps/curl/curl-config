# shellcheck disable=SC2006

prefix="/Users/jszczerbinski/git/libsnowflakeclient/scripts/../deps-build/darwin/Debug/curl"
# Used in ${exec_prefix}/lib
# shellcheck disable=SC2034
exec_prefix=${prefix}
# shellcheck disable=SC2034
includedir=${prefix}/include
cppflag_curl_staticlib=-DCURL_STATICLIB

usage()
{
  cat <<EOF
Usage: curl-config [OPTION]

Available values for OPTION include:

  --built-shared says 'yes' if libcurl was built shared
  --ca                  CA bundle install path
  --cc                  compiler
  --cflags              preprocessor and compiler flags
  --checkfor [version]  check for (lib)curl of the specified version
  --configure           the arguments given to configure when building curl
  --features            newline separated list of enabled features
  --help                display this help and exit
  --libs                library linking information
  --prefix              curl install prefix
  --protocols           newline separated list of enabled protocols
  --ssl-backends        output the SSL backends libcurl was built to support
  --static-libs         static libcurl library linking information
  --version             output version information
  --vernum              output version as a hexadecimal number
EOF

  exit "$1"
}

if test "$#" -eq 0; then
  usage 1
fi

while test "$#" -gt 0; do
  case "$1" in
  --built-shared)
    echo 'no'
    ;;

  --ca)
    echo '"/etc/ssl/cert.pem"'
    ;;

  --cc)
    echo 'clang'
    ;;

  --prefix)
    echo "$prefix"
    ;;

  --feature|--features)
    for feature in alt-svc AsynchDNS Debug HSTS HTTPS-proxy IPv6 Largefile libz NTLM SSL threadsafe TLS-SRP TrackMemory UnixSockets ""; do
      test -n "$feature" && echo "$feature"
    done
    ;;

  --protocols)
    # shellcheck disable=SC2043
    for protocol in HTTP HTTPS IPFS IPNS MQTT; do
      echo "$protocol"
    done
    ;;

  --version)
    echo 'libcurl 8.10.1'
    exit 0
    ;;

  --checkfor)
    checkfor=$2
    cmajor=`echo "$checkfor" | cut -d. -f1`
    cminor=`echo "$checkfor" | cut -d. -f2`
    # when extracting the patch part we strip off everything after a
    # dash as that's used for things like version 1.2.3-pre1
    cpatch=`echo "$checkfor" | cut -d. -f3 | cut -d- -f1`

    vmajor=`echo '8.10.1' | cut -d. -f1`
    vminor=`echo '8.10.1' | cut -d. -f2`
    # when extracting the patch part we strip off everything after a
    # dash as that's used for things like version 1.2.3-pre1
    vpatch=`echo '8.10.1' | cut -d. -f3 | cut -d- -f1`

    if test "$vmajor" -gt "$cmajor"; then
      exit 0
    fi
    if test "$vmajor" -eq "$cmajor"; then
      if test "$vminor" -gt "$cminor"; then
        exit 0
      fi
      if test "$vminor" -eq "$cminor"; then
        if test "$cpatch" -le "$vpatch"; then
          exit 0
        fi
      fi
    fi

    echo "requested version $checkfor is newer than existing 8.10.1"
    exit 1
    ;;

  --vernum)
    echo '080a01'
    exit 0
    ;;

  --help)
    usage 0
    ;;

  --cflags)
    if test "X$cppflag_curl_staticlib" = "X-DCURL_STATICLIB"; then
      CPPFLAG_CURL_STATICLIB="-DCURL_STATICLIB "
    else
      CPPFLAG_CURL_STATICLIB=""
    fi
    if test "X${prefix}/include" = "X/usr/include"; then
      echo "${CPPFLAG_CURL_STATICLIB}"
    else
      echo "${CPPFLAG_CURL_STATICLIB}-I${prefix}/include"
    fi
    ;;

  --libs)
    if test "X${exec_prefix}/lib" != "X/usr/lib" -a "X${exec_prefix}/lib" != "X/usr/lib64"; then
      CURLLIBDIR="-L${exec_prefix}/lib "
    else
      CURLLIBDIR=""
    fi
    if test "Xno" = "Xno"; then
      echo "${CURLLIBDIR}-lcurl -lssl -lcrypto -lz -ltelemetry -ldl"
    else
      echo "${CURLLIBDIR}-lcurl"
    fi
    ;;

  --ssl-backends)
    echo 'OpenSSL v3+'
    ;;

  --static-libs)
    if test "Xyes" != "Xno" ; then
      echo "${exec_prefix}/lib/libcurl.a" -L/Users/jszczerbinski/git/libsnowflakeclient/scripts/../deps-build/darwin/Debug/oob/lib -L/Users/jszczerbinski/git/libsnowflakeclient/scripts/../deps-build/darwin/Debug/zlib/lib -L/Users/jszczerbinski/git/libsnowflakeclient/scripts/../deps-build/darwin/Debug/openssl/lib -framework CoreFoundation -framework CoreServices -framework SystemConfiguration -lssl -lcrypto -lz -ltelemetry -ldl
    else
      echo 'curl was built with static libraries disabled' >&2
      exit 1
    fi
    ;;

  --configure)
    echo " '--enable-debug' '--with-ssl=/Users/jszczerbinski/git/libsnowflakeclient/scripts/../deps-build/darwin/Debug/openssl' '--with-zlib=/Users/jszczerbinski/git/libsnowflakeclient/scripts/../deps-build/darwin/Debug/zlib' '--without-nss' '--enable-static' '--disable-shared' '--prefix=/Users/jszczerbinski/git/libsnowflakeclient/scripts/../deps-build/darwin/Debug/curl' '--without-libssh2' '--without-brotli' '--without-zstd' '--without-libpsl' '--disable-rtsp' '--disable-ldap' '--disable-ldaps' '--disable-telnet' '--disable-tftp' '--disable-imap' '--disable-smb' '--disable-smtp' '--disable-gopher' '--disable-pop3' '--disable-ftp' '--disable-dict' '--disable-file' '--disable-manual' 'CC=clang' 'CFLAGS=-arch arm64 -mmacosx-version-min=10.14' 'LDFLAGS=-L/Users/jszczerbinski/git/libsnowflakeclient/scripts/../deps-build/darwin/Debug/oob/lib' 'LIBS=-ltelemetry -ldl' 'CPPFLAGS=-I/Users/jszczerbinski/git/libsnowflakeclient/scripts/../deps-build/darwin/Debug/oob/include'"
    ;;

  *)
    echo "unknown option: $1"
    usage 1
    ;;
  esac
  shift
done

exit 0
